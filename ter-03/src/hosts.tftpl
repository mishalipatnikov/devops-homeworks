%{~ for i in db ~}
${i["name"]}   ansible_host=${i["network_interface"][0]["nat_ip_address"]} ansible_fqdn=${i["metadata"]["fqdn"]}
%{~ endfor ~}

[storage]

%{~ for i in storage ~}
${i["name"]}   ansible_host=${i["network_interface"][0]["nat_ip_address"]} ansible_fqdn=${i["metadata"]["fqdn"]}
%{~ endfor ~}
